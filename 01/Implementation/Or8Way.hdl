/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    // Put your code here:
	Or(a = in[0], b = in[1], out = v);
	Or(a = v, b = in[2], out = w);
	Or(a = w, b = in[3], out = x);
	Or(a = x, b = in[4], out = y);
	Or(a = y, b = in[5], out = z);
	Or(a = z, b = in[6], out = u);
	Or(a = u, b = in[7], out = out);
}
